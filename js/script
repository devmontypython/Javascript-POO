class Produto {

    constructor() {
        this.id = 1
        
    }

    salvar() {
        let produto = this.lerDados()
    }

    lerDados() {
        let produto = {}

        produto.id = this.id
        produto.nomeProduto = document.getElementById('produto').value
        produto.preco = document.getElementById('preco').value

        return produto
    }

    cancelar() {
        
    }
}

var produto = new Produto()